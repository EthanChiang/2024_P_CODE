def test():
   if not 0:
      print ("always true")


def maxValue(Alist):
   if not Alist:
      return None
   ##   print(test)
   else:
      maxNumber = Alist[0] 
      for item in Alist:
         if maxNumber < item:  
             maxNumber = item
      return maxNumber       

a=[1, 2, 3, 4, 5, 6, 7, 8, 10]

print(maxValue(a))
